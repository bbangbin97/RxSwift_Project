# Uncomment the next line to define a global platform for your project
platform :ios, '12.1'

target 'Install Rx' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  

  # Pods for Install Rx
  pod 'RxSwift', '~> 4.0'
  pod 'RxCocoa', '~> 4.0' 
  pod 'SwiftyJSON'
  pod 'RxSwiftExt'
  pod 'RxAlamofire'
  pod 'RxAnimated'
  
end
